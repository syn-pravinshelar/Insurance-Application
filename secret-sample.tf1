variable "whoami" {
  default = "funbobo"
}

resource "aws_key_pair" "deployer" {
  key_name   = "${var.whoami}_key"
  public_key = ""
}

variable "private_key" {
  default = ""
}

# Change this if you want to deploy new code to ec2 without killing the ec2 instance
variable "codeversion" {
  default = "v1"
}
